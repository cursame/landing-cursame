<div class="wrapper">
	<div class="cols-12">
		<div class="col col-12">
			<div class="col-inner">
				<div class="info">
					<form class="form" action="/mailer" method="POST" enctype="multipart/form-data">
						<h1>Contacto</h1>
						Nombre Completo: <input type="text" name="name" />
						Cargo que actualmente ocupa: <input type="text" name="name" />
						Email de contacto: <input type="text" name="email" />
						Teléfono de la escuela: <input type="text" name="phone" />
						Nombre de la Institución: <input type="text" name="institution" />
						<%= erb :'/static_views/pate/locations/states'%>
						Content: <textarea name="content"></textarea>
						<input type="submit" value="Submit Comment" />
					</form>
				</div>
			</div>
		</div>
	</div>
</div>
