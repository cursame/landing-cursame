<%=  erb :'/static_views/index_renders/social'%>
<div class="plugins bg-darkgray">
  <div class="wrapper">
    <div class="cols-12">
      <h2><%= t.impact.general_impact %></h2>
      <div class="col col-6">
        <div class="col-inner">
          <div class="dashboard">
            <ul class="social_list">
              <li class="color"><%= t.impact.li1 %></li>
              <li class="color"><%= t.impact.li2 %></li>
              <li class="color"><%= t.impact.li3 %></li>
            </ul>
          </div>
        </div>
      </div>
      <div class="col col-6">
        <div class="col-inner">
          <div class="dashboard">
            <ul class="social_list">
              <li class="color"><%= t.impact.li4 %></li>
              <li class="color"><%= t.impact.li5 %></li>
              <li class="color"><%= t.impact.li6 %></li>
              <li class="color"><%= t.impact.li7 %></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="social content-top">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-6">
        <div class="col-inner">
          <div class="info">
            <h2><%= t.impact.impact_teachers %></h2>
            <p><%= t.impact.chart_1_txt %></p>
          </div>
        </div>
      </div>
      <div class="col col-6">
        <div class="col-inner">
          <div class="info">
            <div class="img_center">
              <%= image_tag 'chart1.png'%>
            </div>
          </div>
        </div>
      </div>
    </div>
    <hr/>
  </div>
</div>
<div class="social content-top">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-6">
        <div class="col-inner">
          <div class="info">
            <div class="img_center">
              <%= image_tag 'chart2.png'%>
            </div>
          </div>
        </div>
      </div>
      <div class="col col-6">
        <div class="col-inner">
          <div class="info">
            <h2><%= t.impact.impact_in_education %></h2>
            <p><%= t.impact.chart_2_txt %></p>
          </div>
        </div>
      </div>
    </div>
    <hr/>
  </div>
</div>
<div class="social content-top">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-5">
        <div class="col-inner">
          <div class="info">
            <h2><%= t.impact.impact_in_tech %></h2>
            <p><%= t.impact.chart_3_txt%></p>
          </div>
        </div>
      </div>
      <div class="col col-7">
        <div class="col-inner">
          <div class="info">
            <div class="img_center">
              <%= image_tag 'chart3.png','100%'%>
            </div>
          </div>
        </div>
      </div>
      <hr class="cta">
      <div class="col col-12">
        <div class="col-inner">
          <p class="p-big"></p>
          <div class="buttons_cta">
            <ul class="button_banner">
              <li>
                <%= link_to t.impact.link_public , 'pate/call', '',  'btn btn-sponsor' %>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>