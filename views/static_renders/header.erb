<div class=" <%= 'header' if transparent_head %>  <%= 'header_scroll' if !transparent_head%>">
  <div class="wrapper">
    <div class="cols-12">
      <div class="col col-12">
        <div class="col-inner">
           <div class="logo">
            <% if transparent_head %>
            <img src="/img/mainlogo.png" alt="">
            <%else%>
            <img src="/img/mainlogo_color.png" alt="">
            <%end%>
          </div>
        <div class="menu-home">
          <ul class=" <%= 'menu-main' if transparent_head %> <%= 'menu-main-bottom' if !transparent_head %> nav-top">
            <li><a href="/<%=session[:locale] %>"><%= t.menu.home %></a></li>
            <li><a href="/<%=session[:locale] %>/us"><%= t.menu.us %></a></li>
            <li><a href="/<%=session[:locale] %>/functions"><%= t.menu.functions %></a></li>
            <li><a href="/<%=session[:locale] %>/rse"><%= t.menu.rse %></a></li>
            <li><a href="/<%=session[:locale] %>/plugins"><%= t.menu.plugins %></a></li>
            <li><a href="/<%=session[:locale] %>/benefits"><%= t.menu.benefits %></a></li>
            <li><a href="/<%=session[:locale] %>/contact"><%= t.menu.contact %></a></li>
          </ul>
        </div>

        <div class="menu-mobile totarget" data-target="menu-imobile">
         <ul>
           <li><i class="i i-menu " ></i>lalalala</li>
         </ul>
        </div>
          <div class="login">
            <ul class=" <%= 'singin-button' if transparent_head %>  <%= 'singin-button-bottom' if !transparent_head %>">
              <li><a href="/<%=session[:locale] %>/member"><%= t.menu.already_a_member %></a></li>
              <li><a href="/<%=session[:locale] %>/find_network" class="blue"><%= t.menu.loggin %></a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  </div>
